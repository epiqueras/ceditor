{
  "name": "Ceditor",
  "version": "1.0.0",
  "description": "A simple and easy to use code editor with a built in REPL.",
  "main": "main.js",
  "scripts": {
    "start:electron": "electron ./index.js",
    "start:dev-server": "webpack-dev-server --config webpack.config.dev.js",
    "start": "NODE_ENV=development concurrently -k 'npm run start:dev-server' 'npm run start:electron'",
    "start:prod": "NODE_ENV=production npm run start:electron"
  },
  "repository": "https://github.com/epiqueras/ceditor",
  "keywords": [
    "Ceditor",
    "Text Editor",
    "Code Editor",
    "REPL"
  ],
  "author": "epiqueras",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.18.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "electron-devtools-installer": "^2.0.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "^0.10.0",
    "node-sass": "^4.3.0",
    "postcss": "^5.2.11",
    "redux-devtools": "^3.3.2",
    "redux-devtools-extension": "^1.0.0",
    "redux-immutable-state-invariant": "^1.2.4",
    "redux-unhandled-action": "^1.2.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^15.0.1",
    "stylelint-webpack-plugin": "^0.5.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.1.0-beta.0",
    "react-hot-loader": "^3.0.0-beta.6"
  },
  "dependencies": {
    "codemirror": "^5.23.0",
    "electron": "^1.4.1",
    "electron-config": "^0.2.1",
    "material-design-icons": "^3.0.1",
    "react": "^15.4.2",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0"
  }
}
